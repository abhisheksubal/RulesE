{
  "rules": [
    {
      "ruleId": "rule1",
      "ruleName": "Age Check Rule",
      "type": "expression",
      "conditionExpression": "if(typeOf(age) == 'Int32' || typeOf(age) == 'Double', age > 18, false)",
      "actionExpressions": {
        "status": "'adult'"
      }
    },
    {
      "ruleId": "rule2",
      "ruleName": "Voting Eligibility Rule",
      "type": "expression",
      "conditionExpression": "status == 'adult'",
      "actionExpressions": {
        "canVote": "true"
      }
    }
  ]
} 